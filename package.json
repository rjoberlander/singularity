{
  "name": "singularity",
  "version": "1.0.0",
  "description": "Singularity - AI-Powered Health Protocol Tracking",
  "private": true,
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "dev:api": "cd apps/api && npm run dev",
    "dev:mobile": "cd apps/mobile && npm start",
    "dev:web": "cd apps/web && npm run dev",
    "dev": "concurrently \"npm run dev:api\" \"npm run dev:web\"",
    "build:api": "cd apps/api && npm run build",
    "build:mobile": "cd apps/mobile && npm run build:web",
    "build:web": "cd apps/web && npm run build",
    "build:packages": "npm run build -w @singularity/shared-types && npm run build -w @singularity/shared-utils && npm run build -w @singularity/shared-api",
    "install:all": "npm install && npm run build:packages"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rjoberlander/singularity.git"
  },
  "author": "",
  "license": "UNLICENSED",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/rjoberlander/singularity/issues"
  },
  "homepage": "https://github.com/rjoberlander/singularity#readme",
  "dependencies": {
    "@supabase/supabase-js": "^2.89.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0"
  }
}
